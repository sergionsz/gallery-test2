{
  "name": "gallery-test",
  "version": "1.0.0",
  "description": "A responsive gallery test",
  "main": "app.js",
  "scripts": {
    "prescss": "mkdirp public/css",
    "scss": "node-sass --output-style compressed -o public/css src/scss",
    "autoprefixer": "postcss -u autoprefixer -r public/css/*",
    "build:css": "npm run scss && npm run autoprefixer",
    "prebabel": "mkdirp public/js",
    "babel": "babel src/js --out-dir public/js",
    "uglify": "uglifyjs tmp/js/*.js -m -c -o public/js/app.js",
    "build:js": "npm run babel",
    "preimagemin": "mkdirp public/img",
    "imagemin": "imagemin src/img public/img -p",
    "build": "npm run build:css && npm run build:js && npm run imagemin",
    "test": "tape tests/*.js",
    "start": "mkdirp logs && node ./bin/www.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sergionsz/gallery-test2"
 },
  "keywords": [
    "gallery",
    "test",
    "front-end",
    "responsive"
  ],
  "author": "Sergio NS",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sergionsz/gallery-test2/issues"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "imagemin-cli": "~2.1.0",
    "mkdirp": "^0.5.1",
    "node-sass": "~3.7.0",
    "postcss-cli": "~2.5.2",
    "rimraf": "^2.5.2",
    "tape": "^4.5.1",
    "uglify-js": "~2.6.2"
  },
  "dependencies": {
    "debug": "~2.2.0",
    "dotenv": "~2.0.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "winston": "^2.2.0"
 }
}
